    "pretty-bytes": "^5.4.1",
    "svgo": "^3.3.2",
    "vue": "3.5.24",
    "xml-formatter": "^3.4.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/file-saver": "^2.0.5",
    "@types/node": "20.11.5",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
