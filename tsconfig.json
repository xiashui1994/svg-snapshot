{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "./**/*.ts",
    "./**/*.vue"
  ],
  "compilerOptions": {
    "paths": {
      "~*": [
        "./*"
      ]
    },
    "types": [
      "vue"
    ],
    "lib": [
      "ES2015",
      "DOM",
      "DOM.Iterable"
    ],
    "baseUrl": "."
  }
}